# Migration Feature Flags Configuration
# Copy these to your .env file and adjust values as needed

# Core GTM Migration Flags
REACT_APP_GTM_ENABLED=false
REACT_APP_GTM_PARALLEL_TRACKING=true
REACT_APP_GTM_ROLLOUT_PERCENTAGE=0

# Component-specific Migration Flags
REACT_APP_GTM_CHECKOUT_TRACKING=false
REACT_APP_GTM_PAYMENT_TRACKING=false
REACT_APP_GTM_THANKYOU_TRACKING=false

# Advanced Features
REACT_APP_ENHANCED_CONVERSIONS_ENABLED=false
REACT_APP_SERVER_SIDE_BACKUP_ENABLED=false

# Migration Monitoring
REACT_APP_MIGRATION_MONITORING_ENABLED=true
REACT_APP_CONVERSION_VALIDATION_ENABLED=true

# Rollback Controls
REACT_APP_EMERGENCY_ROLLBACK_ENABLED=false
REACT_APP_LEGACY_TRACKING_FALLBACK=true

# Migration Phases Configuration Guide:
# 
# Phase 1: Setup (0% rollout)
# - GTM_ENABLED=false
# - GTM_PARALLEL_TRACKING=true
# - GTM_ROLLOUT_PERCENTAGE=0
# - All component flags=false
# 
# Phase 2: Testing (5% rollout)
# - GTM_ENABLED=true
# - GTM_PARALLEL_TRACKING=true
# - GTM_ROLLOUT_PERCENTAGE=5
# - All component flags=false
# 
# Phase 3: Gradual Rollout (25% rollout)
# - GTM_ENABLED=true
# - GTM_PARALLEL_TRACKING=true
# - GTM_ROLLOUT_PERCENTAGE=25
# - GTM_CHECKOUT_TRACKING=true
# 
# Phase 4: Expanded Rollout (50% rollout)
# - GTM_ENABLED=true
# - GTM_PARALLEL_TRACKING=true
# - GTM_ROLLOUT_PERCENTAGE=50
# - GTM_CHECKOUT_TRACKING=true
# - GTM_PAYMENT_TRACKING=true
# 
# Phase 5: Full Component Rollout (75% rollout)
# - GTM_ENABLED=true
# - GTM_PARALLEL_TRACKING=true
# - GTM_ROLLOUT_PERCENTAGE=75
# - All component flags=true
# 
# Phase 6: Full Migration (100% rollout)
# - GTM_ENABLED=true
# - GTM_PARALLEL_TRACKING=false (can disable parallel tracking)
# - GTM_ROLLOUT_PERCENTAGE=100
# - All component flags=true
# 
# Emergency Rollback:
# - EMERGENCY_ROLLBACK_ENABLED=true
# - GTM_ENABLED=false
# - LEGACY_TRACKING_FALLBACK=true