# Google Ads API Configuration
# Required for server-side conversion tracking

# Google Ads Developer Token (from Google Ads API Center)
GOOGLE_ADS_DEVELOPER_TOKEN=your_developer_token_here

# OAuth 2.0 Client Credentials (from Google Cloud Console)
GOOGLE_ADS_CLIENT_ID=your_client_id.apps.googleusercontent.com
GOOGLE_ADS_CLIENT_SECRET=your_client_secret

# OAuth 2.0 Refresh Token (generated during OAuth flow)
GOOGLE_ADS_REFRESH_TOKEN=your_refresh_token

# Google Ads Customer ID (without dashes, e.g., 1234567890)
GOOGLE_ADS_CUSTOMER_ID=1234567890

# Conversion Action IDs (from Google Ads interface)
GOOGLE_ADS_PURCHASE_CONVERSION_ACTION=17482092392/AbC-D_efGhIjKlMnOp
GOOGLE_ADS_BEGIN_CHECKOUT_CONVERSION_ACTION=17482092392/XyZ-A_bcDeFgHiJkLm
GOOGLE_ADS_VIEW_ITEM_CONVERSION_ACTION=17482092392/QrS-T_uvWxYzAbCdEf

# Supabase Configuration (automatically provided in Supabase functions)
SUPABASE_URL=https://your-project.supabase.co
SUPABASE_ANON_KEY=your_anon_key
SUPABASE_SERVICE_ROLE_KEY=your_service_role_key